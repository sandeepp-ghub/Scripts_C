             }

         }
         if [file exists IObuff_spread_30.tcl] {
            if { $source_repository_pattern_file == true && [file exists $pattern_repository/[get_db current_design .name]/IObuff_spread_30.tcl] } {
               echo "   (PSI-INFO): Sourcing repository file: $pattern_repository/[get_db current_design .name]/IObuff_spread_30.tcl..."
               source $pattern_repository/[get_db current_design .name]/IObuff_spread_30.tcl
            } else {
               source IObuff_spread_30.tcl
            }; # End of IF;
            move_pattern_to_repository    IObuff_spread_30.tcl [get_db current_design .name]  $pattern_repository only_one
         } else {
            echo "   (PSI-INFO): File: IObuff_spread_30.tcl  NOT found...continuing."
         }; # End of IF;
     }
####################################################################################
# Setting 40% activity factor to: INPUT non-Clock IOBufs
####################################################################################
} elseif { $AF_value2 == "0.4" || $AF_value2 == "0.40" } {
     catch {file delete -force IObuff_spread_40.tcl}

     #JGF#set list2  [get_cells -hier *IencIOBuf*]
     #JGF#set list2_pins [get_pins -quiet -of_objects $list2 -filter "direction == out && is_clock == false"]
     set list2_pins [get_non_clock_ports]

     # Static simulation
     ######################
     if { [string match  {*static_run*} [pwd]] } {
         set_switching_activity -activity 0.4 -pin [list $list2_pins]
     }

     # Dynamic simulation
     ######################
     if { [string match  {*dynamic_run*} [pwd]] || [string match  {*dflow_run_dir*} [pwd]] } {

         # for power_calc
         set_switching_activity -activity 0.4 -pin [list $list2_pins]

         # for dynamic toggle_rate
         echo "   (PSI-INFO): Applying 40% activity factors to IencIOBuf, see all commands and inst names in IObuff_spread_40.tcl"
         foreach pin $list2_pins {
             set is_toggle [expr {round(rand()*25)}]
                 set inst [file dirname $pin]
                 set mypin [file tail $pin]
             if { $is_toggle == 0 } {
                     echo "set_power -dynamic_switch_pattern { r f r f - - - - - - r f r f - - - - - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_40.tcl
             } elseif { $is_toggle == 1 } {
                     echo "set_power -dynamic_switch_pattern { - - r f r f - - - - - - r f r f - - - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_40.tcl
             } elseif { $is_toggle == 2 } {
                     echo "set_power -dynamic_switch_pattern { - - - - r f r r - - - - - - r f r r - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_40.tcl
             } elseif { $is_toggle == 3 } {
                     echo "set_power -dynamic_switch_pattern { - - - - - - r f r f - - - - - - r f r f } -instance $inst -pin $mypin" -repeat >> IObuff_spread_40.tcl
             } elseif { $is_toggle == 4 } {
                     echo "set_power -dynamic_switch_pattern { r f - - - - - - r f r f - - - - - - r f } -instance $inst -pin $mypin" -repeat >> IObuff_spread_40.tcl
             } elseif { $is_toggle == 5 } {
                     echo "set_power -dynamic_switch_pattern { r f r f r f r f - - - - - - - - - - - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_40.tcl
             } elseif { $is_toggle == 6 } {
                     echo "set_power -dynamic_switch_pattern { - - r f r f r f r f - - - - - - - - - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_40.tcl
             } elseif { $is_toggle == 7 } {
                     echo "set_power -dynamic_switch_pattern { - - - - r f r f r f r f - - - - - - - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_40.tcl
             } elseif { $is_toggle == 8 } {
                     echo "set_power -dynamic_switch_pattern { - - - - - - r f r f r f r f - - - - - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_40.tcl
             } elseif { $is_toggle == 9 } {
                     echo "set_power -dynamic_switch_pattern { - - - - - - - - r f r f r f r f - - - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_40.tcl
             } elseif { $is_toggle == 10 } {
                     echo "set_power -dynamic_switch_pattern { - - - - - - - - - - r f r f r f r f - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_40.tcl
             } elseif { $is_toggle == 11 } {
                     echo "set_power -dynamic_switch_pattern { - - - - - - - - - - - - r f r f r f r f } -instance $inst -pin $mypin" -repeat >> IObuff_spread_40.tcl
             } elseif { $is_toggle == 12 } {
                     echo "set_power -dynamic_switch_pattern { r f - - - - - - - - - - - - r f r f r f } -instance $inst -pin $mypin" -repeat >> IObuff_spread_40.tcl
             } elseif { $is_toggle == 13 } {
                     echo "set_power -dynamic_switch_pattern { r f r f - - - - - - - - - - - - r f r f } -instance $inst -pin $mypin" -repeat >> IObuff_spread_40.tcl
             } elseif { $is_toggle == 14 } {
                     echo "set_power -dynamic_switch_pattern { r f r f r f - - - - - - - - - - - - r f } -instance $inst -pin $mypin" -repeat >> IObuff_spread_40.tcl
             } elseif { $is_toggle == 15 } {
                     echo "set_power -dynamic_switch_pattern { r f - - r f - - r f - - r f - - - - - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_40.tcl
             } elseif { $is_toggle == 16 } {
                     echo "set_power -dynamic_switch_pattern { - - r f - - r f - - r f - - r f - - - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_40.tcl
             } elseif { $is_toggle == 17 } {
                     echo "set_power -dynamic_switch_pattern { - - - - r f - - r f - - r f - - r f - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_40.tcl
             } elseif { $is_toggle == 18 } {
