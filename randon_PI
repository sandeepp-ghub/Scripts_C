## Sets de-synchronized switching activity to all non-clock inpu ports of a block.
##      For AF: 10%
##      For AF: 20%
##      For AF: 25%
##      For AF: 30%
##      For AF: 40%
##      For AF: 50%
##      For AF: 60%


redirect -variable -quiet AF_value {puts [get_default_switching_activity -input_activity]}
set AF_value2 [exec -- echo $AF_value | grep input_activity | sed {s/.*activity//g} | sed {s/[{}]//g} | sed {s/ //g} ]
echo "   (PSI-random_PI) - block AF: $AF_value2"

set pattern_repository                       /proj/cayman/DE3/pgv/IObuff_pattern/
set source_repository_pattern_file           true

####################################################################################
# PROC:  get_non_clock_ports
####################################################################################
proc get_non_clock_ports { args } {
   set non_clk_IOBuf_list [list]
   set show_output_ports     0
   set show_clock_ports      1
   set show_non_clock_ports  1
   foreach port [::get_db ports] {

      #### INPUT PORTS ####
      if { [::get_db $port .direction] eq "in" } {

         # CLOCKS #
         if { [llength [::get_db $port .clocks]] > 0 } {
            if { $show_clock_ports } {
               foreach pin [::get_db $port .net.load_pins] {
                  set inst [::get_db $pin .inst]
                  foreach opin [::get_db $inst .pins -if { .direction eq "out" }] {
                     if { $show_clock_ports } {
                        puts [format "      |--> CLOCK PORT: %s (clocks: %s)" [::get_db $opin .name] [::get_db $port .clocks.name]]
                     }; # End of IF;
                  }; # End of FOREACH;
               }; # End of FOREACH;
            }; # End of IF;

         # NON-CLOCKS #
         } else {
            foreach pin [::get_db $port .net.load_pins] {
               set inst [::get_db $pin .inst]
               if { [::get_db $inst .is_buffer] } {
                  foreach opin [::get_db $inst .pins -if { .direction eq "out" }] {
                     #JGF#puts [format "      IencIOBuf:%s" [::get_db $opin .name]]
                     lappend non_clk_IOBuf_list [::get_db $opin .name]
                  }; # End of FOREACH;
               } else {
                  puts "      |--> Port not connected to buffer: [::get_db $port .name]"
               }; # End of IF;
            }; # End of FOREACH;
         }; # End of IF;

      #### OUTPUT PORTS ####
      } else {
         if { ! $show_non_clock_ports } {
            if { [llength [::get_db $port .clocks]] > 0 } {
               if { $show_output_ports } {
                  puts [format "      OUTPUT PORT: %s (clocks: %s)" [::get_db $port .name] [::get_db $port .clocks.name]]
               }; # End of IF;
            }; # End of IF;
         }; # End of IF;
      }; # End of IF;
   }; # END OF FOREACH;
   return $non_clk_IOBuf_list;
}; # End of PROC;
####################################################################################

####################################################################################
# PROC:  move_pattern_to_repository
####################################################################################
proc move_pattern_to_repository { pattern_file_path block_name repository_path {file_action replace}} {

            if { [file isdirectory ${repository_path}] } {
               if { ! [file isdirectory ${repository_path}/${block_name}] } {
                  mkdir -p ${repository_path}/${block_name}
               }; # End of IF;
               if { $file_action == "replace" } {
                  echo "   (PSI-INFO): (Re)placing ${pattern_file_path} in repository..."
                  file copy -force ${pattern_file_path}  ${repository_path}/${block_name}
               } elseif { $file_action == "only_one" && ! [file exists ${repository_path}/${block_name}/${pattern_file_path}]} {
                  echo "   (PSI-INFO): adding copy of ${pattern_file_path} in repository..."
                  file copy -force ${pattern_file_path}  ${repository_path}/${block_name}
               } elseif { $file_action == "only_one" && [file exists ${repository_path}/${block_name}/${pattern_file_path}]} {
                  echo "   (PSI-INFO): File ${pattern_file_path} already in repository..."
               } else {
                  file copy -force ${pattern_file_path}  ${repository_path}/${block_name}
               }; # End of IF;
               catch {exec chmod -f 777 -R ${repository_path}/${block_name}/};
            }; # End of IF;

}; # End of PROC;
####################################################################################




####################################################################################
# Setting 10% activity factor to: INPUT non-Clock IOBufs
####################################################################################
if { $AF_value2 == "0.1" || $AF_value2 == "0.10" } {
     catch {file delete -force IObuff_spread_10.tcl}

     #JGF#set list2  [get_cells -hier *IencIOBuf*]
     #JGF#set list2_pins [get_pins -quiet -of_objects $list2 -filter "direction == out && is_clock == false"]
     set list2_pins [get_non_clock_ports]


     # Static simulation
     ######################
     if { [string match  {*static_run*} [pwd]] } {
         set_switching_activity -activity 0.1 -pin [list $list2_pins]
     }

     # Dynamic simulation
     ######################
     if { [string match  {*dynamic_run*} [pwd]] || [string match  {*dflow_run_dir*} [pwd]] } {

         # for power_calc
         set_switching_activity -activity 0.1 -pin [list $list2_pins]

         # for dynamic toggle_rate
         echo "   (PSI-INFO): Applying 10% activity factors to IencIOBuf, see all commands and inst names in IObuff_spread_10.tcl"
         foreach pin $list2_pins {
             set is_toggle [expr {round(rand()*10)}]
                 set inst [file dirname $pin]
                 set mypin [file tail $pin]
             if { $is_toggle == 0 } {
                     echo "set_power -dynamic_switch_pattern { r f - - - - - - - - - - - - - - - - - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_10.tcl
             } elseif { $is_toggle == 1 } {
                     echo "set_power -dynamic_switch_pattern { - - r f - - - - - - - - - - - - - - - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_10.tcl
             } elseif { $is_toggle == 2 } {
                     echo "set_power -dynamic_switch_pattern { - - - - r f - - - - - - - - - - - - - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_10.tcl
             } elseif { $is_toggle == 3 } {
                     echo "set_power -dynamic_switch_pattern { - - - - - - r f - - - - - - - - - - - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_10.tcl
             } elseif { $is_toggle == 4 } {
                     echo "set_power -dynamic_switch_pattern { - - - - - - - - r f - - - - - - - - - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_10.tcl
             } elseif { $is_toggle == 5 } {
                     echo "set_power -dynamic_switch_pattern { - - - - - - - - - - r f - - - - - - - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_10.tcl
             } elseif { $is_toggle == 6 } {
                     echo "set_power -dynamic_switch_pattern { - - - - - - - - - - - - r f - - - - - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_10.tcl
             } elseif { $is_toggle == 7 } {
                     echo "set_power -dynamic_switch_pattern { - - - - - - - - - - - - - - r f - - - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_10.tcl
             } elseif { $is_toggle == 8 } {
                     echo "set_power -dynamic_switch_pattern { - - - - - - - - - - - - - - - - r f - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_10.tcl
             } else {
                     echo "set_power -dynamic_switch_pattern { - - - - - - - - - - - - - - - - - - r f } -instance $inst -pin $mypin" -repeat >> IObuff_spread_10.tcl
             }

         }
         if [file exists IObuff_spread_10.tcl] {
            if { $source_repository_pattern_file == true && [file exists $pattern_repository/[get_db current_design .name]/IObuff_spread_10.tcl] } {
               echo "   (PSI-INFO): Sourcing repository file: $pattern_repository/[get_db current_design .name]/IObuff_spread_10.tcl..."
               source $pattern_repository/[get_db current_design .name]/IObuff_spread_10.tcl
            } else {
               source IObuff_spread_10.tcl
            }; # End of IF;
            move_pattern_to_repository    IObuff_spread_10.tcl [get_db current_design .name]  $pattern_repository only_one
         } else {
            echo "   (PSI-INFO): File: IObuff_spread_10.tcl  NOT found...continuing."
         }; # End of IF;
     }

####################################################################################
