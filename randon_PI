             } elseif { $is_toggle == 20 } {
                     echo "set_power -dynamic_switch_pattern { r f - - r f - - r f - - - - - - - - - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_30.tcl
             } elseif { $is_toggle == 21 } {
                     echo "set_power -dynamic_switch_pattern { - - r f - - r f - - r f - - - - - - - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_30.tcl
             } elseif { $is_toggle == 22 } {
                     echo "set_power -dynamic_switch_pattern { - - - - r f - - r f - - r f - - - - - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_30.tcl
             } elseif { $is_toggle == 23 } {
                     echo "set_power -dynamic_switch_pattern { - - - - - - r f - - r f - - r f - - - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_30.tcl
             } elseif { $is_toggle == 24 } {
                     echo "set_power -dynamic_switch_pattern { - - - - - - - - r f - - r f - - r f - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_30.tcl
             } elseif { $is_toggle == 25 } {
                     echo "set_power -dynamic_switch_pattern { - - - - - - - - - - r f - - r f - - r f } -instance $inst -pin $mypin" -repeat >> IObuff_spread_30.tcl
             } elseif { $is_toggle == 26 } {
                     echo "set_power -dynamic_switch_pattern { r f - - - - - - - - - - r f - - r f - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_30.tcl
             } elseif { $is_toggle == 27 } {
                     echo "set_power -dynamic_switch_pattern { - - r f - - - - - - - - - - r f - - r f } -instance $inst -pin $mypin" -repeat >> IObuff_spread_30.tcl
             } elseif { $is_toggle == 28 } {
