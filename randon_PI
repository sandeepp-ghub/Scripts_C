             } elseif { $is_toggle == 2 } {
                     echo "set_power -dynamic_switch_pattern { - - - - r f r f r f - - - - - - - - - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_30.tcl
             } elseif { $is_toggle == 3 } {
                     echo "set_power -dynamic_switch_pattern { - - - - - - r f r f r f - - - - - - - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_30.tcl
             } elseif { $is_toggle == 4 } {
                     echo "set_power -dynamic_switch_pattern { - - - - - - - - r f r f r f - - - - - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_30.tcl
             } elseif { $is_toggle == 5 } {
                     echo "set_power -dynamic_switch_pattern { - - - - - - - - - - r f r f r f - - - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_30.tcl
             } elseif { $is_toggle == 6 } {
                     echo "set_power -dynamic_switch_pattern { - - - - - - - - - - - - r f r f r f - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_30.tcl
             } elseif { $is_toggle == 7 } {
                     echo "set_power -dynamic_switch_pattern { - - - - - - - - - - - - - - r f r f r f } -instance $inst -pin $mypin" -repeat >> IObuff_spread_30.tcl
             } elseif { $is_toggle == 8 } {
                     echo "set_power -dynamic_switch_pattern { r f - - - - - - - - - - - - - - r f r f } -instance $inst -pin $mypin" -repeat >> IObuff_spread_30.tcl
             } elseif { $is_toggle == 9 } {
                     echo "set_power -dynamic_switch_pattern { r f r f - - - - - - - - - - - - - - r f } -instance $inst -pin $mypin" -repeat >> IObuff_spread_30.tcl
             } elseif { $is_toggle == 10 } {
                     echo "set_power -dynamic_switch_pattern { r f - - - - r f - - - - r f - - - - - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_30.tcl
             } elseif { $is_toggle == 11 } {
                     echo "set_power -dynamic_switch_pattern { - - r f - - - - r f - - - - r f - - - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_30.tcl
             } elseif { $is_toggle == 12 } {
                     echo "set_power -dynamic_switch_pattern { - - - - r f - - - - r f - - - - r f - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_30.tcl
             } elseif { $is_toggle == 13 } {
                     echo "set_power -dynamic_switch_pattern { - - - - - - r f - - - - r f - - - - r f } -instance $inst -pin $mypin" -repeat >> IObuff_spread_30.tcl
             } elseif { $is_toggle == 14 } {
