                     echo "set_power -dynamic_switch_pattern { r f - - r f - - r f - - r f r f r f - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_60.tcl
             } elseif { $is_toggle == 19 } {
                     echo "set_power -dynamic_switch_pattern { - - r f - - r f - - r f - - r f r f r f } -instance $inst -pin $mypin" -repeat >> IObuff_spread_60.tcl
             } elseif { $is_toggle == 20 } {
                     echo "set_power -dynamic_switch_pattern { r f r f r f - - - - r f r f r f - - - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_60.tcl
             } elseif { $is_toggle == 21 } {
                     echo "set_power -dynamic_switch_pattern { - - r f r f r f - - - - r f r f r f - - } -instance $inst -pin $mypin" -repeat >> IObuff_spread_60.tcl
             } elseif { $is_toggle == 22 } {
                     echo "set_power -dynamic_switch_pattern { - - - - r f r f r f - - - - r f r f r f } -instance $inst -pin $mypin" -repeat >> IObuff_spread_60.tcl
             } elseif { $is_toggle == 23 } {
                     echo "set_power -dynamic_switch_pattern { r f - - - - r f r f r f - - - - r f r f } -instance $inst -pin $mypin" -repeat >> IObuff_spread_60.tcl
             } else {
                     echo "set_power -dynamic_switch_pattern { r f r f - - - - r f r f r f - - - - r f } -instance $inst -pin $mypin" -repeat >> IObuff_spread_60.tcl
             }

         }
         if [file exists IObuff_spread_60.tcl] {
            if { $source_repository_pattern_file == true && [file exists $pattern_repository/[get_db current_design .name]/IObuff_spread_60.tcl] } {
               echo "   (PSI-INFO): Sourcing repository file: $pattern_repository/[get_db current_design .name]/IObuff_spread_60.tcl..."
               source $pattern_repository/[get_db current_design .name]/IObuff_spread_60.tcl
            } else {
               source IObuff_spread_60.tcl
            }; # End of IF;
            move_pattern_to_repository    IObuff_spread_60.tcl [get_db current_design .name]  $pattern_repository only_one
         } else {
            echo "   (PSI-INFO): File: IObuff_spread_60.tcl  NOT found...continuing."
         }; # End of IF;
     }

####################################################################################
#     AF not FOUND
####################################################################################
} else {
    echo "ERROR: no AF set for $AF_value2"
}
