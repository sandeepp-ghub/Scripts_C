set region_area [ list $hbm0_urx $hbm0_lly [expr $hbm0_urx + 75 ] $hbm0_ury ]
create_group -type guide -rects $region_area -name $name
set c1 [ get_db [ get_db [get_db pins *hbm_group_0/armux*_*[*]] .net.loads.inst ] -if {.base_name != *hbm_group_0 } ]
set c2 [ get_db [ get_db [get_db pins *hbm_group_0/armux*_*[*]] .net.drivers.inst ] -if {.base_name != *hbm_group_0 } ]
set c [ lsort -u [ concat $c1 $c2 ]]
update_group -name $name -add -obj [get_db $c]

