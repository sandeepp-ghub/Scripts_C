create_group -type guide -rects $region_area -name $name
set c [ get_db [ get_db insts axi_top/xbar_hbm2fab/g_pipe_mux_dp_*__g_wdata_mux_*__wdata_mux/level_1__column_2__middle_row_mux/SYN* ] -if {.base_cell.name == *MUX*} ]
update_group -name $name -add -obj [get_db $c]

##### Stage L1C1 - Capture both even and odd - Make it land next to L0C3 - Wires will travell horizontally
set name wdata_hbm_l1c1_mux
delete_obj [get_db groups $name ]
foreach { hbm1_llx hbm1_lly hbm1_urx hbm1_ury } [ join [ get_db [ get_db insts axi_top/hbm_group_1 ] .bbox ] ] {}
#set region_area [ list [ expr $hbm1_urx + 200 ] [expr $hbm1_lly + 0 ] [expr $hbm1_urx + 250 ] [expr $hbm1_lly + $midY ]]
