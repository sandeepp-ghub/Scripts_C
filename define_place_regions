### Creating groups for read data
set name hbm2fab__rdata001
delete_obj [get_db groups $name ]
set region_area "750 3287 1077 4713"
create_group -type guide -rects $region_area -name $name
set c1 [ get_db insts axi_top/xbar_hbm2fab/mem_rdata_pipe_reg_0__0__1* ]
set c2 [ get_db insts axi_top/xbar_hbm2fab/mem_rdata_pipe_reg_1__0__1* ]
set c3 [ get_db insts axi_top/xbar_hbm2fab/mem_rdata_pipe_reg_1__1__1* ]
## Write data flop
set c4 [ get_db insts axi_top/xbar_hbm2fab/g_pipe_mux_dp_*__g_wdata_mux_*__wdata_mux/level_3__column_0__last_row_mux/genblk3_ff_stage_1/dn_data_reg* ]
set c [ concat $c1 $c2 $c3 $c4 ]
update_group -name $name -add -obj [get_db $c ]

set name fab2hbm__rdata001
delete_obj [get_db groups $name ]
set region_area "0 3287 310 4713"
create_group -type guide -rects $region_area -name $name
set c1 [ get_db insts axi_top/xbar_fab2hbm/mem_rdata_pipe_reg_0__0__1* ]
set c2 [ get_db insts axi_top/xbar_fab2hbm/mem_rdata_pipe_reg_1__0__1* ]
set c3 [ get_db insts axi_top/xbar_fab2hbm/mem_rdata_pipe_reg_1__1__1* ]
## Write data flop
set c4 [ get_db insts axi_top/xbar_fab2hbm/g_pipe_mux_dp_*__g_wdata_mux_*__wdata_mux/level_3__column_0__last_row_mux/genblk3_ff_stage_1/dn_data_reg* ]
set c [ concat $c1 $c2 $c3 $c4 ]
update_group -name $name -add -obj [get_db $c ]

