update_group -name $name -add -obj [get_db $c]

##### Stage L1C0 - Capture both even and odd - Make it land next below L2C0 - This is limit veritcal wires to 2048 X 2
set name wdata_hbm_l1c0_mux
delete_obj [get_db groups $name ]
foreach { hbm1_llx hbm1_lly hbm1_urx hbm1_ury } [ join [ get_db [ get_db insts axi_top/hbm_group_1 ] .bbox ] ] {}
set region_area [ list [ expr $hbm1_urx + 600 ] [expr $hbm1_ury - $midY/2 ] [expr $hbm1_urx + 700 ] [expr $hbm1_ury + $midY/2 ]]
