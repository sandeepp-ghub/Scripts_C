### Going towards fab0 and fab1
set name rdata_fab0fab1_stage1
delete_obj [get_db groups $name ]
foreach { fab1_llx fab1_lly fab1_urx fab1_ury } [ join [ get_db [ get_db insts axi_top/fab_group_1 ] .bbox ] ] {}
set midY [ expr (($fab1_ury - $fab1_lly)/2) + $fab1_lly ]
set region_area [ list [expr $fab1_llx - 150 ] [expr $midY - 250 ] [expr $fab1_llx - 50 ] [expr $midY + 250 ]]
create_group -type guide -rects $region_area -name $name
set c1 [ get_db insts  axi_top/xbar_hbm2fab/mem_rdata_pipe_reg_2__1__1_* ]
set c2 [ get_db insts  axi_top/xbar_hbm2fab/mem_rdata_pipe_reg_2__0__1_* ]
set c [ concat $c1 $c2]
update_group -name $name -add -obj [get_db $c ]

