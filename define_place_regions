# Stage L0C7 MUX - Capture both even and odd
set name wdata_hbm_l0c7_mux
delete_obj [get_db groups $name ]
foreach { hbm3_llx hbm3_lly hbm3_urx hbm3_ury } [ join [ get_db [ get_db insts axi_top/hbm_group_3 ] .bbox ] ] {}
set midY [ expr (($hbm3_ury - $hbm3_lly)/2) + $hbm3_lly ]
set region_area [ list [ expr $hbm3_urx + 120 ] $hbm3_lly [expr $hbm3_urx + 200 ] [expr $hbm3_lly + $midY ]]
create_group -type guide -rects $region_area -name $name
set c [ get_db [ get_db insts axi_top/xbar_hbm2fab/g_pipe_mux_dp_*__g_wdata_mux_*__wdata_mux/level_0__column_7__first_row_g_more_than_2_ports_mux/SYN* ] -if {.base_cell.name == *MUX*} ]
update_group -name $name -add -obj [get_db $c]

