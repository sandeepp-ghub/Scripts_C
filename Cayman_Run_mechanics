#!/bin/tclsh
#
#
#Fixed clock cells
mdc::check_place
set fix [get_db markers .objects -if {.obj_type==inst}]
set_db $fix .place_status_cts unset
place_detail -eco true -hard_fence false
set_db $fix .place_status_cts fixed


#relaxed
# special derate relaxation
FLOW(sta_relaxing_mode1)
    # Use 850mv derate table for high voltage corners (Currently only supporting Tempus)
FLOW(sta_relaxing_mode2)
    # Remove the aging derate from the hold capture clocks and reduce the additional aging by 0.5% (Supporting Innovus and Tempus)
FLOW(sta_relaxing_mode3)
    # Reduce hold margin by 5(ff)-10(ss) ps.   (Supporting Innovus and Tempus)
FLOW(sta_relaxing_mode4)
    # Setup corners - launch clock 0, data path 2%, capture clock default.

#BBM / bbm
>/proj/cayman/wa/davidlawson/impl/scripts/bbm_verilog_from_log.pl \
/mrvl2g/cayman_extvol09/cayman/cayman/wa_009/jpike/impl/cayman_top.1.0A/track.Jan31_DE3_postDFT_top_fabric_inter_bbm/tempuscui.signoff.sta/logfiles/run_func_setup_ssgnp0p675v0c_cworstCCwT0c_2023-01-31-10:48:11.log| grep ch_vl_bot

/proj/cayman/wa/davidlawson/impl/scripts/bbm_verilog_from_log.pl \
logfiles/run_func_setup_ssgnp0p675v0c_cworstCCwT0c_2023-02-18-22:34:05.log |  grep hbm_xbar_wrap


#Dynamic Runs
 ll -rt track*/pgv.signoff.dynamic/dynamic_run/adsRpt/dynamic.rpt.summary


/proj/cayman/wa/gford/tcl/split_pgv_dynamic_hxw /proj/cayman/wa/esayre/impl/hbm_xbar_wrapper_0.1.0A/track.DE3_PGVspef/pgv.signoff.dynamic.top/dynamic_run/adsRpt
